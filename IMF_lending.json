{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "IMF Lending Outstanding ",
    "subtitle": "Loan disbursed $bn*",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/Linzeeee/Linzeeee.github.io/main/data_IMF_creditOutstanding.csv"
  },
  "height": 400,
  "width": "container",
  "mark": {"type": "bar", "color": "#86d0bb"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "nominal",
      "axis": {"title": null, "grid": false, "ticks": true, "labelAngle": 60}
    },
    "y": {
      "field": "Totals",
      "type": "quantitative",
      "title": "",
      "axis": {"grid": false}
    }
  },
  "config": {}
}